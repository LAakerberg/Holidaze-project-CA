{
  "name": "holidaze-project",
  "private": true,
  "version": "0.6.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 3000",
    "format": "prettier -w index.html && prettier -w src/*.jsx && prettier -w src/**/*.jsx && prettier -w src/**/*.js",
    "lint": "eslint src/App.jsx",
    "lint-fix": "eslint src/**/*.js --cache --fix && eslint src/**/*.jsx --cache --fix",
    "prepare": "husky install",
    "test": "npm run test-e2e-cli",
    "test-e2e": "cypress open",
    "test-e2e-report": "cypress run --reporter mochawesome",
    "test-e2e-cli": "cypress run"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.1.0",
    "react": "^18.2.0",
    "react-calendar": "^4.2.1",
    "react-datepicker": "^4.11.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-icons": "^4.8.0",
    "react-router-dom": "^6.11.2",
    "styled-components": "^5.3.10",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "cypress": "^12.13.0",
    "eslint": "^8.38.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "tailwindcss": "^3.3.1",
    "vite": "^4.2.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.jsx": [
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.css": [
      "prettier --write"
    ]
  }
}
